    "description": "YFSNS 定位模块 - 支持腾讯定位等",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "合肥音符信息科技有限公司",
            "email": "support@yfsns.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Yfsns\\LaravelLocation\\": "src/"
        }
